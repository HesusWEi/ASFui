<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.VBox?>

<TitledPane collapsible="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="262.0"
            prefHeight="400.0" prefWidth="262.0" text="License" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" minHeight="400.0" minWidth="262.0" prefHeight="400.0" prefWidth="262.0" spacing="10.0">
        <Button fx:id="license_add" mnemonicParsing="false" styleClass="normal-button" text="Add">
            <tooltip>
                <Tooltip text="Activates given appIDs (Steam Network) or subIDs (Steam Store) on current bot instance (free games only)."/>
            </tooltip>
        </Button>
        <Button fx:id="license_addall" mnemonicParsing="false" styleClass="normal-button" text="Add All">
            <tooltip>
                <Tooltip text="Activates given appIDs (Steam Network) or subIDs (Steam Store) on all bots instances (free games only)."/>
            </tooltip>
        </Button>
    </VBox>
</TitledPane>
